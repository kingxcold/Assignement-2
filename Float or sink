#include <iostream>
#include <cmath>
#define pi 3.14

using namespace std;

int main()
{
    int weight;
    float radius;
    float fb;
    cout<<"Enter the weight in pound: ";
    cin>>weight;
    cout<<"Enter the radius in feet: ";
    cin>>radius;
    fb=62.4*((4*pi*pow(radius,3))/3);
    if(fb>=weight)
        cout<<"Object will float";
    else
        cout<<"Object will sink";

}
